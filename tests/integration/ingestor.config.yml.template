nexus_file: ''
done_writing_message_file: ''
config_file: ''
id: ''

dataset:
  allow_dataset_pid: true
  generate_dataset_pid: false
  dataset_pid_prefix: 20.500.12269
  default_instrument_id: TEST_INSTRUMENT
  default_proposal_id: TEST_PROPOSAL
  default_owner_group: ingestor
  default_access_groups:
  - ingestor
  - admin

ingestion:
  dry_run: false
  offline_ingestor_executable:
  - scicat_background_ingestor
  max_offline_ingestors: 10
  offline_ingestors_wait_time: 10
  schemas_directory: <SCHEMAS_DIRECTORY>
  check_if_dataset_exists_by_pid: true
  check_if_dataset_exists_by_metadata: false
  check_if_dataset_exists_by_metadata_key: job_id
  file_handling:
    compute_file_stats: true
    compute_file_hash: true
    file_hash_algorithm: blake2b
    save_file_hash: false
    hash_file_extension: b2b
    ingestor_files_directory: ../ingestor
    message_to_file: false
    message_file_extension: message.json
    file_path_type: absolute
    data_file_open_max_tries: 3
    data_file_open_retry_delay: [3, 5, 10]

kafka:
  topics: test-topic
  group_id: scicat-ingestor-dev
  bootstrap_servers: localhost:9093
  security_protocol: plaintext
  sasl_mechanism: PLAIN
  sasl_username: ""
  sasl_password: ""
  ssl_ca_location: ""
  individual_message_commit: false
  enable_auto_commit: true
  auto_offset_reset: earliest

logging:
  verbose: true
  file_log: false
  file_log_base_name: scicat_ingestor_log
  file_log_timestamp: false
  logging_level: DEBUG
  log_message_prefix: SCI-TEST
  system_log: false
  system_log_facility: mail
  graylog: false

scicat:
  host: http://localhost:3000/api/v3
  token: <VALID_TOKEN_HERE>
  additional_headers: {}
  timeout: 30
  stream: true
  verify: false
  api_endpoints:
    datasets: datasets
    proposals: proposals
    origdatablocks: origdatablocks
    instruments: instruments
